{{ '0-custom-accessories.out.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
  }
{%- endstyle -%}

<div class="arete-two-accessories__wrapper section-{{ section.id }}-padding">
  <div class="arete-two-accessories__container">
    <h2>{{ section.settings.title }}</h2>
    <ul>
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'accessories' %}
            {%- if block.settings.product != blank -%}
              <li>
                <p class="benefits-container">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    xmlns:xlink="http://www.w3.org/1999/xlink"
                    width="30"
                    height="30"
                    viewBox="0 0 30 30"
                    fill="none"
                  >
                    <rect width="30" height="30" fill="url(#pattern0_5270_580)"/>
                    <defs>
                    <pattern id="pattern0_5270_580" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_5270_580" transform="scale(0.00217865)"/>
                    </pattern>
                    <image id="image0_5270_580" width="459" height="459" xlink:href="data:image/png;base64,"/>
                    </defs>
                  </svg>
                  <span>{{ block.settings.benefits | truncate: 50 }}</span>
                </p>
                <div class="image-container">
                  {%- if block.settings.image_product != blank and block.settings.show_default_image -%}
                    {{ block.settings.image_product | image_url: width: 280 | image_tag: class: '__image', loading: 'lazy' }}
                  {% else %}
                    {{ block.settings.product.featured_image | image_url: width: 280 | image_tag: class: '__image', loading: 'lazy' }}
                  {%- endif -%}
                </div>
                <div class="text-container">
                  <p>
                    <span>{{ block.settings.product.title | truncate: 15 }}</span>
                    <span>{{- block.settings.product.price | money -}}</span>
                  </p>
                  <a href="/products/{{ block.settings.product.handle }}">Learn more</a>
                </div>
              </li>
            {%- endif -%}
        {% endcase %}
      {% endfor %}
    </ul>
  </div>
</div>

<script>
  document.addEventListener('DOMContentLoaded', () => {});
</script>

{% schema %}
{
  "name": "Custom accesories",
  "tag": "section",
  "class": "page__arete-two-accessories",
  "settings": [
    {
      "type": "header",
      "content": "Header"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 2,
      "unit": "px",
      "label": "Top",
      "default": 30
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 2,
      "unit": "px",
      "label": "Bottom",
      "default": 30
    },
    {
      "type": "text",
      "id": "title",
      "label": "Text for title section"
    }
  ],
  "blocks": [
    {
      "name": "Accessories",
      "type": "accessories",
      "settings": [
        {
          "type": "text",
          "id": "benefits",
          "label": "Text for benefits product"
        },
        {
          "type": "image_picker",
          "id": "image_product",
          "label": "Image of meet (default)",
          "info": "Select an image to replace the original product picture."
        },
        {
          "type": "checkbox",
          "id": "show_default_image",
          "label": "Show default image if available",
          "default": true
        },
        {
          "type": "product",
          "id": "product",
          "label": "Get product",
          "info": "Choose a product. Photo, name and price will be added from the product."
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Custom accesories"
    }
  ]
}
{% endschema %}
