      "settings": {
        "custom_liquid": "{{ 'https://cdn.jsdelivr.net/npm/three@0.160.1/build/three.min.js' | script_tag }}\n\n<div id=\"particles-banner\" style=\"height: 500px; width: 100%; background: #000; position: relative; overflow: hidden;\"></div>\n\n<script>\n// Проверяем, загрузился ли Three.js\nif (typeof THREE === 'undefined') {\n  console.error('Three.js не загрузился');\n  return;\n}\n\n// Создаём сцену\nconst scene = new THREE.Scene();\nscene.background = new THREE.Color(0x0a0a14);\n\n// Камера (ортогональная)\nconst container = document.getElementById('particles-banner');\nconst camera = new THREE.OrthographicCamera(\n  container.clientWidth / -2,\n  container.clientWidth / 2,\n  container.clientHeight / 2,\n  container.clientHeight / -2,\n  1,\n  1000\n);\ncamera.position.z = 10;\n\n// Рендерер\nconst renderer = new THREE.WebGLRenderer({ antialias: true, alpha: true });\nrenderer.setSize(container.clientWidth, container.clientHeight);\nrenderer.setPixelRatio(window.devicePixelRatio);\ncontainer.appendChild(renderer.domElement);\n\n// Частицы\nconst particleCount = 300;\nconst geometry = new THREE.BufferGeometry();\nconst positions = [];\nconst velocities = [];\n\nfor (let i = 0; i < particleCount; i++) {\n  positions.push(\n    (Math.random() - 0.5) * container.clientWidth,\n    (Math.random() - 0.5) * container.clientHeight,\n    0\n  );\n  velocities.push(\n    (Math.random() - 0.5) * 2,\n    (Math.random() - 0.5) * 2,\n    0\n  );\n}\n\ngeometry.setAttribute('position', new THREE.Float32BufferAttribute(positions, 3));\ngeometry.setAttribute('velocity', new THREE.Float32BufferAttribute(velocities, 3));\n\nconst material = new THREE.PointsMaterial({\n  color: 0x00ffff,\n  size: 4,\n  transparent: true,\n  opacity: 0.8\n});\n\nconst points = new THREE.Points(geometry, material);\nscene.add(points);\n\n// Курсор\nlet mouseX = 0, mouseY = 0;\nlet cursorX = 0, cursorY = 0;\n\ncontainer.addEventListener('mousemove', (e) => {\n  const rect = container.getBoundingClientRect();\n  mouseX = e.clientX - rect.left - rect.width / 2;\n  mouseY = -(e.clientY - rect.top - rect.height / 2);\n});\n\n// Анимация\nfunction animate() {\n  requestAnimationFrame(animate);\n\n  const posArray = geometry.attributes.position.array;\n  const velArray = geometry.attributes.velocity.array;\n\n  cursorX += (mouseX - cursorX) * 0.05;\n  cursorY += (mouseY - cursorY) * 0.05;\n\n  for (let i = 0; i < particleCount; i++) {\n    const i3 = i * 3;\n    const dx = posArray[i3] - cursorX;\n    const dy = posArray[i3 + 1] - cursorY;\n    const dist = Math.sqrt(dx * dx + dy * dy);\n\n    if (dist < 100) {\n      const force = (100 - dist) / 100;\n      velArray[i3]     += (dx / dist) * force * 3;\n      velArray[i3 + 1] += (dy / dist) * force * 3;\n    }\n\n    posArray[i3]     += velArray[i3];\n    posArray[i3 + 1] += velArray[i3 + 1];\n\n    velArray[i3]     *= 0.95;\n    velArray[i3 + 1] *= 0.95;\n\n    if (posArray[i3] < -container.clientWidth / 2) posArray[i3] = -container.clientWidth / 2;\n    if (posArray[i3] >  container.clientWidth / 2) posArray[i3] =  container.clientWidth / 2;\n    if (posArray[i3 + 1] < -container.clientHeight / 2) posArray[i3 + 1] = -container.clientHeight / 2;\n    if (posArray[i3 + 1] >  container.clientHeight / 2) posArray[i3 + 1] =  container.clientHeight / 2;\n  }\n\n  geometry.attributes.position.needsUpdate = true;\n  renderer.render(scene, camera);\n}\n\n// Респонсивность\nwindow.addEventListener('resize', () => {\n  const w = container.clientWidth;\n  const h = container.clientHeight;\n  camera.left = -w / 2;\n  camera.right = w / 2;\n  camera.top = h / 2;\n  camera.bottom = -h / 2;\n  camera.updateProjectionMatrix();\n  renderer.setSize(w, h);\n});\n\nanimate();\n</script>",
